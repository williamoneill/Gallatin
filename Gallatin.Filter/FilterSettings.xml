<?xml version="1.0" encoding="utf-8" ?>
<GallatinProxy>
	<Exceptions>
		<Addresses>
			<IP address="192.168.0.1"/>
			<HostName name="foo"/>
			<IP address="10.0.0.1-10.0.0.3"/>
			<IP address="10.0.0.1/24"/>
		</Addresses>
		<Urls>
			<Url name="cnn.com"/>
			<Url name=".gov"/>
		</Urls>
	</Exceptions>
	<Outbound>
		<Banned>
			<IP address="192.168.0.1"/>
			<HostName name="foo"/>
			<IP address="10.0.0.1-10.0.0.3"/>
			<IP address="10.0.0.1/24"/>
		</Banned>
	</Outbound>
	<Inbound>
		<!--  -->
		<BannedKeywords maxWeight="100">
			<Keyword name="pussy" weight="30"/>
		</BannedKeywords>
		<BannedMimeTypes>
			<MimeType name="audo/mpeg"/>
		</BannedMimeTypes>
		<BannedFileExtensions>
			<Extension value=".mp3"/>
		</BannedFileExtensions>
		<VirusContainers>
			<Extension value=".zip" action="drop"/>
		</VirusContainers>
		<!--See http://www.fosi.org/icra/-->
		<ContentRatings>
			<ICRA>

			</ICRA>
			<RSAC></RSAC>
			<evaluWEB></evaluWEB>
			<cyberNOT></cyberNOT>
			<SafeSurf></SafeSurf>
			<Weburbia></Weburbia>

			<Vancouver></Vancouver>
			<ICEC></ICEC>
			<SafeNet></SafeNet>

		</ContentRatings>
	</Inbound>
</GallatinProxy>
