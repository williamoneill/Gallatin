<?xml version="1.0"?>
<configuration>
	<system.diagnostics>
		<trace autoflush="true"/>
		<sources>
			<source name="proxyLog">
				<listeners>
					<add name="Trace.Log"/>
				</listeners>
			</source>
			<source name="webLog">
				<listeners>
					<add name="Trace.Log"/>
				</listeners>
			</source>
			<source name="filterLog">
				<listeners>
					<add name="Trace.Log"/>
				</listeners>
			</source>


			<source name="System.Net">
				<listeners>
					<add name="Trace.Log"/>
				</listeners>
			</source>
			<source name="System.Net.Sockets">
				<listeners>
					<add name="Trace.Log"/>
				</listeners>
			</source>
			<source name="System.Net.Cache">
				<listeners>
					<add name="Trace.Log"/>
				</listeners>
			</source>
			
		</sources>
		<sharedListeners>
			<add name="Trace.Log" type="System.Diagnostics.TextWriterTraceListener" initializeData="Proxy.log.txt"></add>
		</sharedListeners>
		<switches>
			 <!--Values are Error, Warning, Information, and Verbose. Increasing verbosity will negatively affect performance.--> 
			<add name="proxyLog" value="Verbose"/>
			<add name="webLog" value="Warning"/>
			<add name="filterLog" value="Warning"/>

			<add name="System.Net" value="Error" />
			<add name="System.Net.Sockets" value="Error" />
			<add name="System.Net.Cache" value="Error" />
		</switches>
	</system.diagnostics>

</configuration>
